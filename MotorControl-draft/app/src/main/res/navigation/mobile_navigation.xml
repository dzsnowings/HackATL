<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.motorcontrol.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_call"
        android:name="com.example.motorcontrol.CallFragment"
        android:label="@string/call"
        tools:layout="@layout/fragment_call" />

    <fragment
        android:id="@+id/navigation_message"
        android:name="com.example.motorcontrol.ui.message.MessageFragment"
        android:label="@string/message"
        tools:layout="@layout/fragment_message" />

    <fragment
        android:id="@+id/navigation_music"
        android:name="com.example.motorcontrol.ui.message.MessageFragment"
        android:label="@string/music"
        tools:layout="@layout/fragment_music" />
</navigation>